{
  "name": "overpassql-syntax",
  "publisher": "tqdv",
  "version": "1.1.0",

  "engines": {
    "vscodium": "^1.9.0",
    "vscode": "^1.9.0"
  },
  
  "displayName": "Overpass QL syntax",
  "description": "Overpass QL syntax highlighting for vscodium-compatible editors",
  "license": "Artistic-2.0",
  "categories": ["Programming Languages"],
  "keywords": ["overpass", "osm", "openstreetmap"],

  "homepage": "https://github.com/tqdv/vscodium-overpassql-syntax",
  "bugs": {
    "url": "https://github.com/tqdv/vscodium-overpassql-syntax/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tqdv/vscodium-overpassql-syntax.git"
  },

  "contributes": {
    "languages": [
      {
        "id": "overpassql",
        "aliases": ["Overpass QL", "overpassQL", "Overpass Query Language"],
        "extensions": [".overpassql"],
        "configuration": "./overpassql.language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "overpassql",
        "scopeName": "source.overpassql",
        "path": "./overpassql.tmGrammar.json"
      }
    ]
  }
}
